{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabiolages/CursoTypescript.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Fabiano Lages",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabiolages/CursoTypescript/issues"
  },
  "homepage": "https://github.com/fabiolages/CursoTypescript#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "module-alias": "^2.2.3"
  }
}
